from sklearn.ensemble import RandomForestRegressor
import numpy as np
import pandas as pd
#from sklearn.datasets import load_boston
#boston = load_boston()
data = pd.read_excel("training.xlsx")
data.head()
data1 = pd.read_excel("testing.xlsx")
data1.head()
rf = RandomForestRegressor(n_estimators = 500, n_jobs=2)
feature_cols = ['word_count', 'sentence_count', 'spell_error', 'noun_count', 'adjective_count', 'verb_count']
feature_cols1=['total_score']
X = np.array(data[feature_cols])
y=np.array(data[feature_cols1])
rf.fit(X, y)

print "Instance 1 prediction:", rf.predict(X[1])
print "Instance 2 prediction:", rf.predict(X[2])
